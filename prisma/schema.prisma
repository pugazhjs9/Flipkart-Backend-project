datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  createdAt DateTime @default(now())
}

model Song {
  id        Int      @id @default(autoincrement())
  title     String
  artist    String
  duration  Int
  userId    Int
}
